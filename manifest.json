{
    "name": "screen_capture_tunde",
    "description": "screen capture for automatically capturing screen, still in development",
    "version": "1.0",
    "manifest_version": 3,
    "icons":{"128":"camera_128.png", "64":"camera_64.png"},
    "action":{
        "default_icon":"camera_default.png",
        "default_popup":"uu_pp.html"

    },
    "background":{
        "service_worker":"background.js"
        
    },
    "permissions":["storage", "activeTab", "scripting"]
   
}